pipeline {
    agent {
        label 'Agent_Part1_Config'  // Specify the agent configuration
    }
    
    stages {
        stage('Run amey.sh Script') {
            steps {
                script {
                    // Ensure that the script is executable
                    sh 'chmod +x amey.sh'   // Optional, depending on your file's permissions

                    // Run the amey.sh script
                    sh './amey.sh'
                }
            }
        }
    }
    
    post {
        always {
            // Any cleanup or post actions
            echo 'Pipeline execution complete.'
        }
    }
}
